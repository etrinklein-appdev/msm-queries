<h1>List of all actors</h1>

<table border="1">
<% @list_of_actors.each do |blah| %>
  <tr>
    <td><%= blah.id %> </td>
    <td> <img src ="http://robohash.org/<%= blah.name %>"> </td>
    <td><%= blah.name %> </td>
    <td><%= blah.dob %> </td>
    <td> <a href="/actors/<%= blah.id%>">Show Details</a>   </td>
  </tr>
<% end %>